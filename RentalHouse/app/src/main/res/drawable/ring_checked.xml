<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval"
            android:useLevel="false">
            <size
                android:width="17dp"
                android:height="17dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/orange_fda"/>
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <size
                android:width="10dp"
                android:height="10dp"/>
            <solid android:color="@color/orange_fda"/>
        </shape>
    </item>

</layer-list>